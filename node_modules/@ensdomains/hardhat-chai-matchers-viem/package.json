{
  "name": "@ensdomains/hardhat-chai-matchers-viem",
  "type": "module",
  "version": "0.1.14",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./behaviour": {
      "types": "./dist/behaviour.d.ts",
      "import": "./dist/behaviour.js",
      "default": "./dist/behaviour.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "behaviour": [
        "./dist/behaviour.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "src/",
    "!src/**/*.test.ts",
    "!src/test"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "bun run clean && tsc --project tsconfig.build.json",
    "prepublish": "bun run build",
    "compile": "hardhat compile",
    "test": "NODE_OPTIONS=\"--disable-warning=ExperimentalWarning\" vitest"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "3.0.0",
    "@nomicfoundation/hardhat-viem": "3.0.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@types/bun": "latest",
    "hardhat": "3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "viem": "^2.28.1",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "@nomicfoundation/hardhat-viem": "^3.0.0",
    "hardhat": "^3.0.0",
    "viem": "^2.28.1",
    "vitest": "^3.1.2"
  }
}
